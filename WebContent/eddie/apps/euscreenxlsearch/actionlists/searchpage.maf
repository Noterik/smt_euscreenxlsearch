setUrlTrigger(/lou/euscreen/search.html,/lou/domain/webtv/html5application/euscreenxlsearch)
loadContent(screen,favicon)
loadStylesheet(screen,elements)
loadStylesheet(screen,specific)
loadStylesheet(screen,additions)
setRole(screen,"itempage")
callServer(createTypeChunking,screen,content)
callServer(setDefaultType,screen,content)

.when device=desktop
loadContent(screen,template)
loadContent(screen,footer)
loadContent(screen,results)
loadContent(screen,tabs)
