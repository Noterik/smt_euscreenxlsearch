.when device=aphone
callServer(setMobile,screen)
loadContent(screen,mobiletemplate)
loadContent(screen,mediaselector)
loadContent(screen,mobileresults)
loadContent(screen,mobilesearchinput)
loadContent(screen,headerhider)

.when device=atablet
callServer(setTablet,screen)
loadContent(screen,template)
loadContent(screen,tabletresults)
loadContent(screen,tabs)
loadContent(screen,searchinput)
removeContent(screen,footer)

loadContent(screen,header)
loadContent(screen,mobilenav)
loadContent(screen,activefields)
loadContent(screen,filter)
loadContent(screen,resulttopbar)
loadContent(screen,resultcounter)
loadContent(screen,resultsorter)
loadContent(screen,history)
loadContent(screen,linkinterceptor)
callServer(init,screen,content)
